<view>
  <view class='tabBar'>
    <view class='tabBarTitle'>
      <text>我的</text>
    </view>
  </view>
  <view class='head'>
    <image class='avatar' src='/image/icon_me_avatar.png' />
    <text class='name'>{{ userName }}</text>
    <text class='title' style='padding-left: 40px;padding-right: 40px;'>{{ companyName }}</text>
  </view>
  <view class='info'>
    <view class='item'>
      <text class='txt1'>{{ totalTaskCompleteCnt }}个</text>
      <text class='title'>总任务完成数</text>
    </view>
    <view class='item'>
      <text class='txt2'>{{ rate }}%</text>
      <text class='title'>外呼完成率</text>
    </view>
    <view class='item'>
      <text class='txt3'>第 {{rank}} 名</text>
      <text class='title'>团队排名</text>
    </view>
  </view>
  <view class='feedback' bindtap='openFeedback'>
    <image class='icon' src='/image/icon_me_feedback.png' />
    <text class='value'>意见反馈</text>
    <image class='arrow' src='/image/icon_common_arrow.png' />
  </view>
  <view class='about' bindtap='openAbout'>
    <image class='icon' src='/image/icon_me_about.png' />
    <text class='value'>关于闪电呼</text>
    <image class='arrow' src='/image/icon_common_arrow.png' />
  </view>

  <view class='logout_button' bindtap="logout">退出</view>
</view>