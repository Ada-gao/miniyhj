<view class="page">
  <view class="page__bd">
    <!-- 通话结果 -->
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">通话时长</view>
        </view>
        <view class="weui-cell__bd">
          <input class="weui-input" placeholder="" />
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">外呼结果</view>
        </view>
        <view class="weui-cell__bd">
          <picker bindchange="bindCountryChange" value="{{resultIndex}}" range="{{resultsColumns}}" range-key="label">
            <view class="weui-select weui-select_in-select-after">
              {{resultsColumns[resultIndex].label}}
            </view>
          </picker>
        </view>
      </view>
      <view class="weui-cell weui-cell_input">
        <view class="weui-cell__hd">
          <view class="weui-label">下一步行动计划</view>
        </view>
        <view class="weui-cell__bd">
          <picker bindchange="bindActionChange" value="{{actionIndex}}" range="{{actionColumns}}" range-key="label">
            <view class="weui-select weui-select_in-select-after">
              {{actionColumns[actionIndex].label}}
            </view>
          </picker>
        </view>
      </view>
    </view>
    <!-- 客户信息 -->
    <view class="shadow">
      <view class="weui-cells__title">客户信息</view>
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">姓名</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入" />
          </view>
        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">电话</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入" />
          </view>
        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">微信</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入" />
          </view>
        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">备注</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" placeholder="请输入" />
          </view>
        </view>
      </view>
    </view>
    <button class="button" bindtap="openToast">提交</button>
