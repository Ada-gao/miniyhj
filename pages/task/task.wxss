.weui-navbar {
  border: none;
  border-radius: 6rpx;
  font-size: 28rpx;
}

.weui-navbar__item {
  background: #fff;
  padding: 10rpx 0;
  color: #9c9c9c;
  border: 1rpx solid #ccc;
}

.weui-navbar__item:first-child {
  border-radius: 6rpx 0 0 6rpx;
}

.weui-navbar__item:last-child {
  border-radius: 0 6rpx 6rpx 0;
}

.weui-bar__item_on {
  border: none;
  background: linear-gradient(-90deg, rgba(47, 107, 226, 1), rgba(93, 144, 244, 1));
  color: #fff !important;
}

.weui-tab__content .weui-cells .weui-cell {
  padding-left: 0;
  padding-right: 0;
  border-bottom: 1rpx solid #e9e9e9;
  height: 93rpx;
  line-height: 93rpx;
  box-sizing: border-box;
}

