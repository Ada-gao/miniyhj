.head {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 63rpx;
  margin-right: 67rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
}

.complete {
  height: 16rpx;
  background: #e7e7e7;
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
}

.empty {
  height: 16rpx;
  background: #5c8ff2;
  border-radius: 8px;
  margin-left: -5rpx;
}

.snake {
  width: 51rpx;
  height: 51rpx;
  margin-left: -4rpx;
}

.info {
  display: flex;
  flex-direction: row;
}

.item {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
}

.value {
  font-size: 34rpx;
  color: #3570e4;
  font-weight: bold;
}

.title {
  font-size: 24rpx;
  margin-top: 18rpx;
  color: #9c9c9c;
}

.task {
  height: 600rpx;
  margin-left: 60rpx;
  margin-right: 60rpx;
  margin-top: 40rpx;
  border-radius: 12px;
  box-shadow: 6px 4px 20px rgba(169, 169, 169, 0.4);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.emptyItem {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.emptyicon {
  width: 206rpx;
  height: 184rpx;
}

.emptyicon {
  width: 206rpx;
  height: 184rpx;
}

.emptyTxt1 {
  font-size: 32rpx;
  color: #c2c2c2;
  margin-top: 35rpx;
  margin-bottom: 29rpx;
}

.emptyTxt2 {
  font-size: 24rpx;
  color: #d3d3d3;
}

.button {
  margin-left: 60rpx;
  margin-right: 60rpx;
  margin-top: 60rpx;
  line-height: 98rpx;
  background: linear-gradient(-90deg, rgba(47, 107, 226, 1), rgba(93, 144, 244, 1));
  border-radius: 12px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.txt {
  color: #fff;
  font-size: 36rpx;
}

.icon {
  width: 48rpx;
  height: 48rpx;
  margin-right: 22rpx;
}
