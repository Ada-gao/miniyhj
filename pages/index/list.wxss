page{
  background: #f4f5f7
}

.list{
  margin-top: 20rpx;
}

.module {
  background: #fff;
  display: flex;
  flex-direction: row;
  height: 94rpx;
  align-items: center;
  padding-left: 20px;
  padding-right: 20px;
  border-bottom: 1rpx #f5f5f5 solid;
}

.module .name {
  font-size: 32rpx;
  color: #323232;
  flex: 1;
}

.module .value {
  font-size: 26rpx;
  margin-right: 20rpx;
  color: #9c9c9c;
}

.module .arrow {
  width: 33rpx;
  height: 33rpx;
}

.empty {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 300rpx;
}

.empty .icon {
  width: 243rpx;
  height: 158rpx;
}

.empty .txt {
  font-size: 32rpx;
  color: #c2c2c2;
  margin-top: 35rpx;
}
