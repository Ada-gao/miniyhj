<view>
  <view class='tabBar'>
    <view class='tabBarBack' bindtap='back'>
      <image class='tabBarIcon' src='/image/icon_common_back.png'></image>
    </view>
    <view class='tabBarTitle'>
      <text>销售姓名</text>
    </view>
  </view>
  <form bindsubmit="formSubmit">
    <view class='module' style='margin-top:20rpx;'>
      <text class='name'>销售名称</text>
      <input class='value' placeholder='销售名称' maxlength='10' value='{{userName}}' name='username'/>
    </view>
    <button class='button' formType="submit">提交</button>
  </form>
</view>