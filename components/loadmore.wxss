.more {
  height: 94rpx;
  line-height: 94rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding-left: 40rpx;
  padding-right: 40rpx;
}

.more text {
  font-size: 26rpx;
  color: #c0c0c0;
}

.more .line {
  border: 1rpx solid #e9e9e9;
  flex: 1;
  margin-left: 30rpx;
  margin-right: 30rpx;
}